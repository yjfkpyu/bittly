{
  "name": "bittly",
  "version": "0.19.1",
  "author": "sige",
  "private": true,
  "scripts": {
    "publish": "PowerShell -File ./scripts/build-prod-packages.ps1",
    "build:test:package": "PowerShell -File ./scripts/build-test-package.ps1",
    "test:webdriver:start": "chromedriver --log-level=INFO",
    "test:functional": "vue-cli-service test:unit --testMatch \"**/__tests__/functional/**/*.test.js\"",
    "electron:build": "vue-cli-service electron:build",
    "electron:serve": "vue-cli-service electron:serve",
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "test:unit": "vue-cli-service test:unit",
    "test:debug": "vue-cli-service test:unit --runInBand --testTimeout=3600000 --testPathPattern=./src/modules/directive/response/plotter/__tests__/PlotterWebGL.spec.js --testNamePattern=debug",
    "lint": "vue-cli-service lint",
    "postinstall": "electron-builder install-app-deps",
    "postuninstall": "electron-builder install-app-deps",
    "rebuild": "electron-rebuild"
  },
  "main": "background.js",
  "dependencies": {
    "@amap/amap-jsapi-loader": "^1.0.1",
    "@electron/remote": "^2.0.5",
    "@faker-js/faker": "^7.3.0",
    "@svgdotjs/svg.js": "^3.1.2",
    "ant-design-vue": "^1.7.8",
    "art-template": "^4.13.2",
    "axios": "^0.26.1",
    "bluetooth-serial-port": "git+https://gitee.com/sigek/node-bluetooth-serial-port.git",
    "buffer": "^6.0.3",
    "bufferutil": "^4.0.7",
    "core-js": "^3.6.5",
    "crc": "^4.3.2",
    "echarts": "^5.3.2",
    "echarts-liquidfill": "^3.1.0",
    "electron-updater": "^4.6.5",
    "encoding": "^0.1.13",
    "exceljs": "^4.3.0",
    "fengari": "^0.1.4",
    "flush-promises": "^1.0.2",
    "https": "^1.0.0",
    "jest-css-modules": "^2.1.0",
    "jspdf": "^2.5.1",
    "jspdf-autotable": "^3.5.23",
    "jszip": "^3.10.1",
    "litegraph.js": "^0.7.10",
    "mathjs": "^11.7.0",
    "modbus-serial": "git+https://github.com/yaacov/node-modbus-serial.git",
    "mqtt": "^4.3.7",
    "pem": "^1.14.6",
    "serialport": "^10.5.0",
    "sqlite3": "^5.0.2",
    "three": "^0.142.0",
    "utf-8-validate": "^5.0.10",
    "uuid": "^8.3.2",
    "vue": "^2.6.11",
    "vue-codemirror": "^4.0.6",
    "vue-draggable-resizable": "^2.3.0",
    "vue-i18n": "^8.27.1",
    "vue-json-viewer": "^2.2.22",
    "vue-shortkey": "^3.1.7",
    "vue-virtual-scroll-list": "^2.3.4",
    "vuedraggable": "^2.24.3",
    "vuejs-thermometer": "^0.1.3",
    "vuex": "^3.4.0",
    "webgl-plot": "^0.7.0",
    "ws": "^8.8.0",
    "xterm": "^4.18.0",
    "xterm-addon-fit": "^0.5.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.16.11",
    "@jest-runner/electron": "^3.0.1",
    "@vue/cli-plugin-babel": "~4.5.13",
    "@vue/cli-plugin-eslint": "~4.5.13",
    "@vue/cli-plugin-unit-jest": "~4.5.13",
    "@vue/cli-service": "~4.5.13",
    "@vue/test-utils": "^1.3.0",
    "babel-eslint": "^10.1.0",
    "babel-preset-env": "^1.7.0",
    "electron": "^17.0.0",
    "electron-chromedriver": "^17.0.0",
    "electron-devtools-installer": "^3.1.0",
    "electron-rebuild": "^3.2.7",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.2.2",
    "jest": "^27.5.1",
    "jest-canvas-mock": "^2.4.0",
    "jest-websocket-mock": "^2.3.0",
    "selenium-webdriver": "^4.4.0",
    "vue-cli-plugin-electron-builder": "~2.1.1",
    "vue-template-compiler": "^2.6.11"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {
      "no-debugger": "off",
      "no-console": "off",
      "no-unused-vars": "off",
      "no-undef": "off",
      "no-empty": "off",
      "no-constant-condition": [
        "error",
        {
          "checkLoops": false
        }
      ],
      "vue/no-unused-components": "off"
    },
    "overrides": [
      {
        "files": [
          "**/__tests__/*.{j,t}s?(x)",
          "**/tests/unit/**/*.spec.{j,t}s?(x)"
        ],
        "env": {
          "jest": true
        }
      }
    ]
  },
  "browserslist": [
    "Chrome 98"
  ]
}
